import java.util.Scanner;

public class SomandoMultiplos {

	public static void main (String[] args) {
		int A,N;
		int somaA = 0;
		int res;
		Scanner input = new Scanner(System.in);
        A = input.nextInt();
        N = input.nextInt();
      
	    for(int i = A; i < N; i++) {
	        
	            somaA =A+ N;
	        
	    }
	    res = somaA;

	    System.out.println("A soma dos multiplos : " +res);
	}
	}

