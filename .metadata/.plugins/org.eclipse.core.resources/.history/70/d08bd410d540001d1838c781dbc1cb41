import java.util.Scanner;

public class SomandoMultiplos {

	public static void main (String[] args) {
		int A,N;
		Scanner input = new Scanner(System.in);
       A = input.nextInt();
       N = input.nextInt();
       int somaA = 0;
	   
	    int res;

	   
	    for(int i = A; i < N; i++) {
	        
	            somaA += N;
	        
	    }
	    res = somaA;

	    System.out.println("A soma dos multiplos : " +res);
	}
	}

