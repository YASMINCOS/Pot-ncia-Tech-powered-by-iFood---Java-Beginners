public class SomandoMultiplos {

	public static void main (String[] args) {
		int A,N;
		Scanner input = new Scanner(System.in);
       A = input.nextInt();
       N = input.nextInt();
       int somaA = 0;
	    int somaN = 0;
	   
	    int res;

	    for(int i = 1; i < A; i++) {
	        if(i % A == 0) {
	            somaA += i;
	        }
	    }
	    for(int i = 1; i < N; i++) {
	        if(i % N == 0) {
	            somaN += i;
	        }
	    }
	    res = somaA + somaN;

	    System.out.println("A soma dos multiplos : " +res);
	}
	}

