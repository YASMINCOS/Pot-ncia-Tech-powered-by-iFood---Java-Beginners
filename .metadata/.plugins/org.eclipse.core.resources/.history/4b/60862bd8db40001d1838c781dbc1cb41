import java.util.Scanner;

public class Recursao {

	public static void main(String[] args) {

       int teste= somatorio(6);
       System.out.println(teste);

    }

    
    static int somatorio(int numero) {
        if (numero == 0) {
            return 1;
        }
            return numero *somatorio(numero - 1);
        
    }
}
